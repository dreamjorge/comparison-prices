{
  "name": "comparison-prices-android-amd64",
  "build": {
    "dockerfile": "../container/Dockerfile",
    "context": ".."
  },
  "runArgs": ["--platform=linux/amd64"],
  "workspaceFolder": "/workspace/app-android",
  "workspaceMount": "source=${localWorkspaceFolder},target=/workspace/app-android,type=bind,consistency=cached",
  "remoteUser": "root",
  "customizations": {
    "vscode": {
      "extensions": [
        "vscjava.vscode-java-pack",
        "vscjava.vscode-gradle",
        "ms-androidtools.vscode-android"
      ]
    }
  }
}
